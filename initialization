def add2Plus(KareemAnswer):
    return 2 + int (KareemAnswer)

print('Hello Kareem, lets do some addition today.')
print('What is your favorite number?')

KareemAnswer = input()

print(add2Plus(KareemAnswer))

def multiply10by(kareemMultiply):
    return 10 * kareemMultiply

